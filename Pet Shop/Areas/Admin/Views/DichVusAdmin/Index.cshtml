@model IEnumerable<Pet_Shop.Models.DichVu>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin_PetShop.cshtml";
}
<h2>Dịch vụ</h2>
<p>
    @Html.ActionLink("Tạo mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenDV)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DoiTuongKD.MoTa)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenDV)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DoiTuongKD.MoTa)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.MaDV }) |
            @Html.ActionLink("Chi tiết", "Details", new { id=item.MaDV }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.MaDV })
        </td>
    </tr>
}

</table>
