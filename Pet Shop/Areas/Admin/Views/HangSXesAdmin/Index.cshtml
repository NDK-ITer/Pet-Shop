@model IEnumerable<Pet_Shop.Models.HangSX>

@{
    ViewBag.Title = "Hãng Sản Xuất";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin_PetShop.cshtml";
}

<h2>Hãng sản xuất</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenNSX)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DiaChi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SuatXu)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenNSX)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiaChi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SuatXu)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MaNSX }) |
            @Html.ActionLink("Details", "Details", new { id=item.MaNSX }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MaNSX })
        </td>
    </tr>
}

</table>
